<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox fx:id="rootVBox" alignment="CENTER" spacing="20.0" stylesheets="@loading.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.burnknuckle.ui.subParts.LoadingPageController">
   <children>
      <Label fx:id="progressLabel" maxHeight="20.0" maxWidth="200.0" prefHeight="20.0" prefWidth="200.0" text="Loading...">
         <font>
            <Font name="Fira Code Regular" size="14.0" />
         </font>
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <ProgressBar fx:id="progressBar" styleClass="progress-bar" prefWidth="200.0" progress="0.0" />
         </children>
   <padding>
      <Insets bottom="50" left="50" right="50" top="50" />
   </padding>
</VBox>
